{
  "name": "react-xarrows",
  "version": "2.0.2",
  "author": "Eliav Louski",
  "description": "Draw arrows (or lines) between components in React!",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "typesVersions": {
    "*": {
      "*": ["dist/types/redesign/*"]
    }
  },
  "scripts": {
    "build:nocheck": "vite build && tsc",
    "build": "tsc -p tsconfig.check.json && vite build && tsc",
    "build:watch": "vite build --config ./vite.config.watch.ts",
    "prepublishOnly": "pnpm run build"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.js",
      "types": "./dist/types/redesign/*.d.ts"
    }
  },

  "homepage": "https://eliav2.github.io/react-xarrows/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Eliav2/react-xarrows"
  },
  "bugs": "https://github.com/Eliav2/react-xarrows/issues",
  "dependencies": {
    "@types/prop-types": "^15.7.3",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react-fast-compare": "^3.2.0",
    "shared": "*"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/react": "^18.0.0",
    "concurrently": "^7.2.1",
    "react": "^18.2.0",
    "rollup": "^3.7.5",
    "rollup-plugin-delete": "^2.0.0",
    "vite": "^3.2.3",
    "vite-plugin-dts": "^1.7.1"
  },
  "funding": {
    "type": "individual",
    "url": "https://www.paypal.com/donate?hosted_button_id=CRQ343F9VTRS8"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || >=18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || >=18.0.0"
  },
  "license": "MIT",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "react",
    "reactjs",
    "line",
    "lines",
    "arrow",
    "arrows",
    "xarrows",
    "draw",
    "connect",
    "between",
    "elements",
    "element"
  ],
  "engines": {
    "node": ">=12.0.0"
  }

}
