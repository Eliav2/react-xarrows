    "dist",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "react",
    "reactjs",
    "line",
    "lines",
    "arrow",
    "arrows",
    "xarrows",
    "draw",
    "connect",
    "between",
    "elements",
    "element"
  ]
